{
  "name": "wappuradio-unofficial",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "npm run lint:fix && npm run prettify",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "prettify": "prettier --config ./prettier.config.mjs --write src *.mjs *.ts",
    "release": "npm run build && rsync -avuP dist/ anssisirencodes:nginx/wappuradio && for i in develop master; do git push origin \"$i\"; done",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/styled": "11.11.5",
    "@fontsource/roboto": "5.0.13",
    "@mui/icons-material": "5.15.15",
    "@mui/material": "5.15.15",
    "@mui/material-nextjs": "5.15.11",
    "mime-types": "2.1.35",
    "next": "14.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.33.3",
    "tailwindcss": "3.4.3"
  },
  "devDependencies": {
    "@emotion/react": "11.11.4",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  }
}
